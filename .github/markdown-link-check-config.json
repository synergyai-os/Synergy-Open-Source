{
  "ignorePatterns": [
    {
      "pattern": "^http"
    },
    {
      "pattern": "^https"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "\\.md",
      "replacement": ""
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}

